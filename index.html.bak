			})
			var nestNRF = d3.nest()
				.key(function(d) { return d.subcycle; }).sortKeys(d3.ascending)
				.rollup(function(v) {
					return v.length;
				})			
				.entries(filterVals)
			nrfChart(nestNRF);
				
